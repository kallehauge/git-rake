{
  "name": "git-rake",
  "version": "1.0.0",
  "description": "Interactive CLI tool to safely prune, delete, and restore Git branches",
  "author": "Andr√© Kallehauge <hi@andrekh.com> (https://andrekh.com/)",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "git-rake": "./bin/git-rake.js"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "DEV=true tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src --fix",
    "lint:check": "eslint src",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
  "keywords": [
    "git",
    "branch",
    "cleanup",
    "cli",
    "interactive",
    "terminal"
  ],
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "cosmiconfig": "^9.0.0",
    "date-fns": "^2.30.0",
    "execa": "^8.0.1",
    "fullscreen-ink": "^0.1.0",
    "fuse.js": "^7.0.0",
    "ink": "^4.4.1",
    "ink-spinner": "^5.0.0",
    "pino": "^9.7.0",
    "react": "^18.2.0",
    "simple-git": "^3.20.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/jest": "^29.5.7",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.37",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "react-devtools-core": "^4.28.5",
    "ts-jest": "^29.1.1",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.1.4",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=14"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run lint"
    ]
  }
}
