{
  "name": "git-rake",
  "version": "1.0.0",
  "description": "Interactive CLI tool to safely prune, delete, and restore Git branches",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "git-rake": "./bin/git-rake.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "git",
    "branch",
    "cleanup",
    "cli",
    "interactive",
    "terminal"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "cosmiconfig": "^9.0.0",
    "date-fns": "^2.30.0",
    "execa": "^8.0.1",
    "fuse.js": "^7.0.0",
    "ink": "^4.4.1",
    "ink-spinner": "^5.0.0",
    "react": "^18.2.0",
    "simple-git": "^3.20.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.7",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.37",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.1.4",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=14"
  }
}
