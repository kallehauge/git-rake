{
  "name": "git-rake",
  "version": "0.1.0",
  "description": "Interactive CLI tool to safely prune, delete, and restore Git branches",
  "author": "Andr√© Kallehauge <hi@andrekh.com> (https://andrekh.com/)",
  "homepage": "https://github.com/kallehauge/git-rake#readme",
  "repository": "kallehauge/git-rake",
  "bugs": "https://github.com/kallehauge/git-rake/issues",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "git-rake": "./bin/git-rake.js"
  },
  "scripts": {
    "build": "tsup src/index.ts --platform node --target node18 --format esm --clean --minify",
    "dev": "DEV=true tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src --fix",
    "lint:check": "eslint src",
    "typecheck": "tsc --noEmit",
    "analyze": "find dist -name '*.js' -exec du -h {} + | sort -rh",
    "prepare": "husky"
  },
  "keywords": [
    "git",
    "branch",
    "cleanup",
    "cli",
    "interactive",
    "terminal"
  ],
  "license": "MIT",
  "dependencies": {
    "commander": "^14.0.0",
    "cosmiconfig": "^9.0.0",
    "date-fns": "^4.1.0",
    "fullscreen-ink": "^0.1.0",
    "ink": "5.2.1",
    "ink-spinner": "^5.0.0",
    "pino": "^9.7.0",
    "react": "^18.2.0",
    "simple-git": "^3.28.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.0",
    "@types/react": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "6.0.0-rc.1",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "lint-staged": "^16.1.4",
    "prettier": "^3.6.2",
    "react-devtools-core": "^4.28.5",
    "ts-jest": "^29.2.5",
    "tsc-alias": "^1.8.16",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.0",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "engines": {
    "node": ">=18"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run lint"
    ]
  }
}
